{
  "name": "css-azure-app-service",
  "version": "0.0.1",
  "description": "Run the Community Solid Server in an Azure App Service",
  "repository": "git@github.com:matthieubosquet/css-azure-app-service.git",
  "type": "module",
  "main": "./index.js",
  "scripts": {
    "start": "node . --config ./config/config.json --baseUrl \"https://$WEBSITE_HOSTNAME\" --rootFilePath /data --mainModulePath . --loggingLevel info --port $PORT",
    "start:dev": "node . --config ./config/config.json --rootFilePath data/ --mainModulePath . --loggingLevel info --port 8080"
  },
  "files": [
    "config"
  ],
  "dependencies": {
    "@solid/community-server": "^8.0.0-alpha.1"
  },
  "license": "MIT"
}
